---
# yamllint disable rule:line-length
lookup_options:
  jenkinsx::casc:
    merge:
      strategy: deep
      merge_hash_arrays: false
      sort_merged_arrays: false
      knockout_prefix: --
  files:
    merge:
      strategy: deep
jenkinsx::casc:
  jenkins:
    securityRealm:
      github:
        clientID: ENC[PKCS7,]
        clientSecret: ENC[PKCS7,]
        githubApiUri: https://api.github.com
        githubWebUri: https://github.com
        oauthScopes: read:org,user:email
  --credentials:
#  credentials:
#    system:
#      domainCredentials:
#        - credentials:
#            - usernamePassword:
#                id: dockerhub-lsstsadmin
#                scope: GLOBAL
#                description: dockerhub (lssttsadmin)
#                username: ENC[PKCS7,]
#                password: ENC[PKCS7,]
files:
  /var/lib/jenkins/init.groovy.d/mailer.groovy:
    content: |
      import hudson.model.*;
      import jenkins.model.*;

      def j = Jenkins.getInstance()
      def ms = j.getDescriptor("hudson.tasks.Mailer")
      ms.setSmtpAuth(
        'ENC[PKCS7,]',
        'ENC[PKCS7,]',
      )
      j.save()
    owner: jenkins
    group: jenkins
    mode: '0440'
    notify: Service[jenkins]
