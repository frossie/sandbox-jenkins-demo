---
# yamllint disable rule:line-length
lookup_options:
  classes:
  jenkins::plugin_hash:
    merge:
      strategy: hash
  jenkinsx::casc:
    merge:
      strategy: deep
      merge_hash_arrays: false
      sort_merged_arrays: false
  files:
    merge:
      strategy: deep
jenkins::plugin_hash:
  ant:
    version: '1.9'
  mapdb-api:
    version: '1.0.9.0'
  resource-disposer:
    version: '0.12'
  subversion:
    version: '2.12.1'
  ws-cleanup:
    version: '0.37'
  robot:
    version: '1.6.5'
jenkinsx::casc:
  jenkins:
  unclassified:
    githubpluginconfig:
      configs:
        - credentialsId: "github-pat-lssttsadmin-webhooks"
    mailer:
      # casc is presently unable to configure smtp auth
      smtpHost: email-smtp.us-west-2.amazonaws.com
      useSsl: true
      smtpPort: 465
      charset: UTF-8
  credentials:
    system:
      domainCredentials:
        - credentials:
            - string:
                id: github-pat-lssttsadmin-webhooks
                scope: GLOBAL
                description: github PAT to manage lsst-ts webhooks
                secret: ENC[PKCS7,]
            - usernamePassword:
                id: dockerhub-lsstsadmin
                scope: GLOBAL
                description: dockerhub (lssttsadmin)
                username: ENC[PKCS7,]
                password: ENC[PKCS7,]
files:
  /var/lib/jenkins/init.groovy.d:
    ensure: directory
    owner: jenkins
    group: jenkins
    mode: '0755'
  /var/lib/jenkins/init.groovy.d/mailer.groovy:
    content: |
      import hudson.model.*;
      import jenkins.model.*;

      def j = Jenkins.getInstance()
      def ms = j.getDescriptor("hudson.tasks.Mailer")
      ms.setSmtpAuth(
        'ENC[PKCS7,]',
        'ENC[PKCS7,]',
      )
      j.save()
    owner: jenkins
    group: jenkins
    mode: '0440'
    notify: Service[jenkins]
