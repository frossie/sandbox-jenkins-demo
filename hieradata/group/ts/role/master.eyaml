---
# yamllint disable rule:line-length
lookup_options:
  classes:
  jenkins::plugin_hash:
    merge:
      strategy: hash
  jenkinsx::casc:
    merge:
      strategy: deep
      merge_hash_arrays: false
      sort_merged_arrays: false
jenkins::plugin_hash:
  ant:
    version: '1.9'
  mapdb-api:
    version: '1.0.9.0'
  resource-disposer:
    version: '0.12'
  subversion:
    version: '2.12.1'
  ws-cleanup:
    version: '0.37'
  robot:
    version: '1.6.5'
jenkinsx::casc:
  jenkins:
  unclassified:
    githubpluginconfig:
      configs:
        - credentialsId: "github-pat-lssttsadmin-webhooks"
  credentials:
    system:
      domainCredentials:
        - credentials:
            - string:
                id: github-pat-lssttsadmin-webhooks
                scope: GLOBAL
                description: github PAT to manage lsst-ts webhooks
                secret: ENC[PKCS7,]
