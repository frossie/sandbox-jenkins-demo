jenkinsx:
  ssh_private_key: &jenkinsx_ssh_private_key |
    ENC[PKCS7,]
# the ssh keypair used to connect to osx slaves is different from the regular
# key pair due to the greater potential of the private key leaking from the
# system jenkins role account.
jenkinsx::osx:
  ssh_private_key: |
    ENC[PKCS7,]
  # the public key value is not used from hiera, it is here only as
  # documentation.
  ssh_public_key: |
    ssh-rsa  jenkins-osx
jenkinsx::authorization_strategy:
  org.jenkinsci.plugins.GithubAuthorizationStrategy:
      arguments:
        - 'jhoblitt, frossie, jonathansick, jmatt'
        - false
        - false
        - false
        - 'lsst, lsst-sqre, lsst-dm'
        - false
        - false
        - false
        - true
jenkinsx::security_realm:
  org.jenkinsci.plugins.GithubSecurityRealm:
    arguments:
      - https://github.com
      - https://api.github.com
      - ENC[PKCS7,]
      - ENC[PKCS7,]
      - read:org
jenkinsx::credentials:
  jenkins-admin-ssh-private-key:
    domain: null
    scope: SYSTEM
    impl: BasicSSHUserPrivateKey
    description: 'jenkins admin'
    username: ENC[PKCS7,]
    private_key: ENC[PKCS7,]
  github-api-token-jhoblitt:
    domain: null
    scope: SYSTEM
    impl: StringCredentialsImpl
    description: 'github API personal access token (jhoblitt)'
    secret: ENC[PKCS7,]
  github-api-token-sqreadmin:
    domain: null
    scope: GLOBAL
    impl: StringCredentialsImpl
    description: 'github API personal access token (sqreadmin)'
    secret: ENC[PKCS7,]
  squash-api-url:
    domain: null
    scope: GLOBAL
    impl: StringCredentialsImpl
    description: 'URL of Squash API endpoint'
    secret: https://%{::squash_fqdn}/dashboard/api
  squash-api-user:
    domain: null
    scope: GLOBAL
    impl: UsernamePasswordCredentialsImpl
    description: 'user/pass for Squash API endpoint'
    username: ENC[PKCS7,]
    password: ENC[PKCS7,]
  jenkins-aws:
    domain: null
    scope: GLOBAL
    impl: UsernamePasswordCredentialsImpl
    description: 'jenkins AWS credentialst'
    username: ENC[PKCS7,]
    password: ENC[PKCS7,]
jenkins::plugins::hipchat:
    token: ENC[PKCS7,]
    room: 'Test Bot: Xenkins'
    sendas: Jenkins
