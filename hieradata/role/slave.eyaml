---
classes:
  - jenkins_demo::role::slave
  - rvm
lsststack::install_convenience: true
# XXX these keys must be duplicated between master & slave roles
jenkins::slave::version: '2.2'
jenkins::slave::source: "https://repo.jenkins-ci.org/releases/org/jenkins-ci/plugins/swarm-client/2.2/"
jenkins::slave::ui_user: ENC[PKCS7,]
jenkins::slave::ui_pass: ENC[PKCS7,]
rvm::system_rubies:
  'ruby-2.2':
    default_use: true
rvm::system_users:
  - jenkins-slave
rvm::rvm_gems:
  'bundler':
    name: 'bundler'
    ruby_version: 'ruby-2.2'
    ensure: '1.13.6'
