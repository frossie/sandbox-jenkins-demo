---
# yamllint disable rule:line-length
lookup_options:
  jenkinsx::casc:
    merge:
      strategy: deep
      merge_hash_arrays: false
      sort_merged_arrays: false
jenkinsx::versiondb:
  ssh_private_key: &jenkinsx_versiondb_ssh_private_key |
    ENC[PKCS7,]
  ssh_public_key: |
    ssh-rsa  jhoblitt-curly@versiondb
jenkinsx::casc:
  jenkins:
    securityRealm:
      github:
        clientID: ENC[PKCS7,]
        clientSecret: ENC[PKCS7,]
        githubApiUri: https://api.github.com
        githubWebUri: https://github.com
        oauthScopes: read:org
  credentials:
    system:
      domainCredentials:
        - credentials:
            - string:
                id: slack-default-channel
                scope: GLOBAL
                description: Default slack channel
                secret: dmj-jhoblitt-curly
            - string:
                id: slack-channel-prefix
                scope: GLOBAL
                description: Prefix for generated slack channels
                secret: dmjjc
            - string:
                id: github-api-token-sqreadmin
                scope: GLOBAL
                description: github API personal access token (sqreadmincurly)
                secret: ENC[PKCS7,]
            - basicSSHUserPrivateKey:
                id: github-jenkins-versiondb
                scope: GLOBAL
                description: github provingground-curly/versiondb deploy key
                username: git
                privateKeySource:
                  directEntry:
                    privateKey: *jenkinsx_versiondb_ssh_private_key
