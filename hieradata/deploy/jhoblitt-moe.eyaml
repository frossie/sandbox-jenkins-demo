---
# yamllint disable rule:line-length
lookup_options:
  jenkinsx::credentials:
    merge: hash
jenkinsx::security_realm:
  org.jenkinsci.plugins.GithubSecurityRealm:
    arguments:
      - https://github.com
      - https://api.github.com
      - ENC[PKCS7,]
      - ENC[PKCS7,]
      - read:org
jenkinsx:versiondb:
  ssh_private_key: &jenkinsx_versiondb_ssh_private_key |
    ENC[PKCS7,]
  # the public key value is not used from hiera, it is here only as
  # documentation.
  ssh_public_key: |
    ssh-rsa  jhoblitt-moe@versiondb
jenkinsx::credentials:
  slack-default-channel:
    domain:
    scope: GLOBAL
    impl: StringCredentialsImpl
    description: Default slack channel
    secret: &slack_default_channel 'dmj-jhoblitt-moe'
  slack-channel-prefix:
    domain:
    scope: GLOBAL
    impl: StringCredentialsImpl
    description: Prefix for generated slack channels
    secret: 'dmjjm'
  github-jenkins-versiondb:
    domain: null
    scope: GLOBAL
    impl: BasicSSHUserPrivateKey
    description: 'github provingground-moe/versiondb deploy key'
    username: ENC[PKCS7,]
    private_key: ENC[PKCS7,]
jenkins::plugins::slack:
  teamdomain: lsstc
  tokencredentialid: slack-lsstc-token
  room: *slack_default_channel
