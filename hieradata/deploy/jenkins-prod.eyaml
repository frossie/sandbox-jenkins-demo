---
# yamllint disable rule:line-length
lookup_options:
  jenkinsx::casc:
    merge:
      strategy: deep
      merge_hash_arrays: false
      sort_merged_arrays: false
jenkinsx:versiondb:
  ssh_private_key: &jenkinsx_versiondb_ssh_private_key |
    ENC[PKCS7,]
  # the public key value is not used from hiera, it is here only as
  # documentation.
  ssh_public_key: |
    ssh-rsa  jenkins-versiondb
jenkinsx::osx:
  ssh_private_key: &jenkinsx_osx_ssh_private_key |
    ENC[PKCS7,]
  # the public key value is not used from hiera, it is here only as
  # documentation.
  ssh_public_key: |
    ssh-rsa  jenkins-osx
jenkinsx::casc:
  jenkins:
    securityRealm:
      github:
        clientID: ENC[PKCS7,]
        clientSecret: ENC[PKCS7,]
        githubApiUri: https://api.github.com
        githubWebUri: https://github.com
        oauthScopes: read:org
    nodes:
      - permanent: &osx_node
          labelString: 'osx osx-10.12 sierra-1'
          mode: EXCLUSIVE
          name: sierra-1
          numExecutors: 1
          remoteFS: /Users/square/j
          launcher:
            sSHLauncher: &osx_ssh
              credentialsId: sqre-osx
              host: jenkins-sierra-1.lsst.cloud
              launchTimeoutSeconds: 210
              maxNumRetries: 0
              port: 22
              retryWaitTime: 15
          nodeProperties:
            - envVars:
                env:
                  - key: 'PATH+LOCAL_BIN'
                    value: /usr/local/bin
      - permanent:
          <<: *osx_node
          name: sierra-2
          labelString: 'osx osx-10.12 sierra-2'
          launcher:
            sSHLauncher:
              <<: *osx_ssh
              host: jenkins-sierra-2.lsst.cloud
      - permanent:
          <<: *osx_node
          name: sierra-3
          labelString: 'osx osx-10.12 sierra-3'
          launcher:
            sSHLauncher:
              <<: *osx_ssh
              host: jenkins-sierra-3.lsst.cloud
      - permanent:
          <<: *osx_node
          name: sierra-4
          labelString: 'osx osx-10.12 sierra-4'
          launcher:
            sSHLauncher:
              <<: *osx_ssh
              host: jenkins-sierra-4.lsst.cloud
  credentials:
    system:
      domainCredentials:
        - credentials:
            - basicSSHUserPrivateKey:
                id: github-jenkins-versiondb
                scope: GLOBAL
                description: 'github lsst/versiondb deploy key'
                username: git
                privateKeySource:
                  directEntry:
                    privateKey: *jenkinsx_versiondb_ssh_private_key
            - basicSSHUserPrivateKey:
                id: sqre-osx
                scope: GLOBAL
                description: 'SQRE OSX build agents'
                username: ENC[PKCS7,]
                privateKeySource:
                  directEntry:
                    privateKey: *jenkinsx_osx_ssh_private_key
            - string:
                id: slack-default-channel
                scope: GLOBAL
                description: 'Default slack channel'
                secret: dm-jenkins
            - string:
                id: slack-channel-prefix
                scope: GLOBAL
                description: 'Prefix for generated slack channels'
                secret: dmj
