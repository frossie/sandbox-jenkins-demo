---
lookup_options:
  jenkinsx::credentials:
    merge: hash
jenkinsx::security_realm:
  org.jenkinsci.plugins.GithubSecurityRealm:
    arguments:
      - https://github.com
      - https://api.github.com
      - ENC[PKCS7,]
      - ENC[PKCS7,]
      - read:org
jenkinsx::credentials:
  slack-default-channel:
    domain:
    scope: GLOBAL
    impl: StringCredentialsImpl
    description: Default slack channel
    secret: &slack_default_channel 'dm-jenkins'
  slack-channel-prefix:
    domain:
    scope: GLOBAL
    impl: StringCredentialsImpl
    description: Prefix for generated slack channels
    secret: 'dmj'
jenkinsx::nodes:
  elcapitan-1:
    template: "%{module_name}/nodes/osx/config.xml.epp"
    params:
      name: elcap-1
      host: jenkins-elcap-1.lsst.cloud
      label: osx osx-10.11 elcap-1
  elcapitan-2:
    template: "%{module_name}/nodes/osx/config.xml.epp"
    params:
      name: elcap-2
      host: jenkins-elcap-2.lsst.cloud
      label: osx osx-10.11 elcap-2
  elcapitan-3:
    template: "%{module_name}/nodes/osx/config.xml.epp"
    params:
      name: elcap-3
      host: jenkins-elcap-3.lsst.cloud
      label: osx osx-10.11 elcap-3
  sierra-1:
    template: "%{module_name}/nodes/osx/config.xml.epp"
    params:
      name: sierra-1
      host: jenkins-sierra-1.lsst.cloud
      label: osx osx-10.12 sierra-1
   # yamllint disable rule:comments-indentation
   # sierra-2:
