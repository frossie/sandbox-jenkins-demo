---
# yamllint disable rule:line-length
lookup_options:
  jenkinsx::credentials:
    merge: hash
jenkinsx::security_realm:
  org.jenkinsci.plugins.GithubSecurityRealm:
    arguments:
      - https://github.com
      - https://api.github.com
      - ENC[PKCS7,]
      - ENC[PKCS7,]
      - read:org
jenkinsx:versiondb:
  ssh_private_key: &jenkinsx_versiondb_ssh_private_key |
    ENC[PKCS7,]
  # the public key value is not used from hiera, it is here only as
  # documentation.
  ssh_public_key: |
    ssh-rsa  jenkins-versiondb
jenkinsx::credentials:
  slack-default-channel:
    domain:
    scope: GLOBAL
    impl: StringCredentialsImpl
    description: Default slack channel
    secret: &slack_default_channel 'dm-jenkins'
  slack-channel-prefix:
    domain:
    scope: GLOBAL
    impl: StringCredentialsImpl
    description: Prefix for generated slack channels
    secret: 'dmj'
  github-jenkins-versiondb:
    domain: null
    scope: GLOBAL
    impl: BasicSSHUserPrivateKey
    description: 'github lsst/versiondb deploy key'
    username: ENC[PKCS7,]
    private_key: ENC[PKCS7,]
jenkinsx::nodes:
  sierra-1:
    template: "%{module_name}/nodes/osx/config.xml.epp"
    params:
      name: sierra-1
      host: jenkins-sierra-1.lsst.cloud
      label: osx osx-10.12 sierra-1
   # yamllint disable rule:comments-indentation
   # sierra-2:
   #   template: "%{module_name}/nodes/osx/config.xml.epp"
   #   params:
   #     name: sierra-2
   #     host: jenkins-sierra-2.lsst.cloud
   #     label: osx osx-10.12 sierra-2
  sierra-3:
    template: "%{module_name}/nodes/osx/config.xml.epp"
    params:
      name: sierra-3
      host: jenkins-sierra-3.lsst.cloud
      label: osx osx-10.12 sierra-3
